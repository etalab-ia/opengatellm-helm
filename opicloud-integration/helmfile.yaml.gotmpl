environments:
  poc: {}
---
repositories:
  - name: elastic
    url: https://helm.elastic.co
  - name: vllm-stack
    url: "https://vllm-project.github.io/production-stack"
  - name: redis-ha
    url: https://dandydeveloper.github.io/charts/
---
releases:
  - name: opengatellm-stack
    namespace: {{ .Namespace }}
    chart: ../charts/opengatellm-stack/
    values:
      - ./values.yaml.gotmpl
  - name: elasticsearch
    namespace: {{ .Namespace }}
    chart: elastic/elasticsearch
    values:
      - ./values-elasticsearch.yaml.gotmpl
